{
  "name": "payment-processing-service",
  "version": "1.0.0",
  "description": "Payment issue processing service with automated decision engine",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx src/worker.ts",
    "worker:dev": "tsx watch src/worker.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
        "typecheck": "tsc --noEmit",
    "process-samples": "tsx scripts/process-sample-issues.ts",
    "ingest-samples": "tsx scripts/ingest-sample-issues.ts"
  },
  "keywords": [
    "payment",
    "processing",
    "fastify",
    "typescript",
    "bullmq"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.7",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "bullmq": "^5.66.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.6.2",
    "ioredis": "^5.9.1",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.8",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@vitest/coverage-v8": "^4.0.17",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
